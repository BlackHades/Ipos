<xs:restriction base="xs:string">

<xs:pattern value=".*"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="Condition">


<xs:restriction base="xs:string">

<xs:minLength value="1"/>

</xs:restriction>

</xs:simpleType>


